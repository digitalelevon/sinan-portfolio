{
    "headers": [
        {
            "source": "/_next/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.+\\.(?:webp|png|jpg|jpeg|svg|ico|gif|avif))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=2592000, stale-while-revalidate=86400"
                }
            ]
        },
        {
            "source": "/(.+\\.(?:woff|woff2|ttf|otf))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}